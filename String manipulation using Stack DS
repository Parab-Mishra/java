import java.util.*;
import java.lang.*;

public class Main{
    private int arr[];
    private int capacity;
    private int top;
    
     Main(int size){
        arr = new int[size];
        capacity = size;
        top = -1;
    }
    
    public Boolean isFull(){
        return top == capacity-1;
    }
    
    public Boolean isEmpty(){
        return top == -1;
    }
    
    public void push(int x){
        if(isFull()){
            System.out.println("Stack is Full !!");
            System.exit(1);
        }
        arr[++top]=x;
    }
    
    public int pop(int x){
        if(isEmpty()){
            System.out.println("Stack is Empty !!");
            System.exit(1);
        }
        return arr[top--];
    }
    
    public void printStack(){
        for(int i =0; i<=top; i++){
            System.out.println((char)arr[i]);
        }
    }
    
    public void printStackRev(){
        for(int i =top; i>=0; i--){
            System.out.println((char)arr[i]);
        }
    }
    
    public void strToStack(String s){
        for(int i = 0; i<=capacity-1; i++){
            push(s.charAt(i));
        }
    }
    
     public static void main(String []args){
         Scanner sc = new Scanner(System.in);
        System.out.println("Enter A String : ");
        String s=sc.nextLine();
        Main stack = new Main(s.length());
        
        stack.strToStack(s);
        
        stack.printStack();
        
    System.out.println();
    stack.printStackRev();
     }
}
